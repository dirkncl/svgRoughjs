{
  "name": "svg2roughjs-sample",
  "description": "A simple sample application to test and try svg2roughjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "cd ../ && npm pack",
    "forceUpdateSvg2roughjs": "npm run preinstall && npm install --force svg2roughjs",
    "start": "npm run forceUpdateSvg2roughjs && webpack-dev-server --open",
    "build": "npm run forceUpdateSvg2roughjs && webpack",
    "deploy": "npm run forceUpdateSvg2roughjs && webpack --mode production"
  },
  "author": "Fabian Schwarzkopf",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "svg2roughjs": "file:../svg2roughjs-1.6.2.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
